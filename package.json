{
  "name": "jira-description-action",
  "version": "0.0.1",
  "description": "Add JIRA issue details to your GitHub pull request",
  "main": "lib/index.js",
  "scripts": {
    "build": "engines-ok && ncc build src/main.ts -o lib -m",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettier": "prettier --write '**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cakeinpanic/jira-description-action"
  },
  "engines": {
    "node": ">= 13",
    "npm": ">= 6"
  },
  "keywords": [
    "actions",
    "github-actions",
    "pr-title",
    "node",
    "setup",
    "github",
    "jira-summary",
    "jira",
    "jira-issue"
  ],
  "author": "cakeinpanic",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^1.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@zeit/ncc": "^0.22.1",
    "axios": "^0.19.2",
    "engines-ok": "^1.2.0",
    "jest": "^25.3.0",
    "jest-circus": "^25.3.0",
    "lint-staged": "^10.1.5",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "prettier": "^2.0.4"
  }
}
